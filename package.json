{
  "name": "vinycherry",
  "version": "0.1.0",
  "author": "Tony Dugué",
  "description": "Application de type mediathèque pour les supports musicaux et plus particulièrement les vinyles",
  "homepage": "https://github.com/tony-dugue/vinycherry",
  "private": true,
  "license": "MIT",
  "keywords": [],
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": ">=24.0.0 <25",
    "yarn": ">=4.12.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "format": "prettier  \"{apps,libs}/**/*.{ts,tsx,js,json}\" --ignore-path .prettierignore",
    "format:check": "yarn format --check",
    "format:write": "yarn format --write"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
